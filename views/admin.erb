<h1>管理者画面</h1>
<a href="/add_questions">問題追加</a>
<% if !@menters.nil? %>
  <% @menters.each do |menter| %>
    <div>
      <%= menter.name %><br>
      <img url="<%= menter.img %>" src="menter_img"  width="40" height="40"><br>
      <%= menter.question %>
      <ul>
        <li><%= menter.one %></li>
        <li><%= menter.two %></li>
        <li><%= menter.three %></li>
        <li><%= menter.four %></li>
      </ul>
      <a href="/change/<%= menter.id %>">更新</a>
    </div><!--問題一覧-->
  <% end %>

<% end %>